auth_from_sel=Només els hosts llistats
auth_command=Permet només l'execució de l'ordre
auth_noport=Permet el reenviament de ports
auth_nox11=permet el reenviament X11
auth_noagent=Permet el reenviament d'agents
auth_nopty=Permet la ubicació de PTYs
auth_err=No he pogut desar la clau autoritzada
auth_ename=Hi falta el nom de la clau o bé és invàlid
auth_ebits=Hi falta el nombre de bits o bé és invàlid
auth_eexp=Hi falta l'exponent de la clau o bé és invàlid
auth_ekey=Hi falten els dígits de la clau o bé són invàlids
auth_ekey2=Hi falten els dígits del text de la clau o bé són invàlids
auth_keytype=Tipus de clau SSH
